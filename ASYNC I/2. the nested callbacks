function callbackHell() {
  setTimeout(() => {
    console.log("First callback");
    setTimeout(() => {
      console.log("Second callback");
      setTimeout(() => {
        console.log("Third callback");
        setTimeout(() => {
          console.log("Fourth callback");
          console.log("Callback hell completed");
        }, 1000);
      }, 1000);
    }, 1000);
  }, 1000);
}

callbackHell();
